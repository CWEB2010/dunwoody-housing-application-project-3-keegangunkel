<Window x:Class="Project_3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="username_box" HorizontalAlignment="Left" Height="23" Margin="589,198,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="180" TextChanged="username_box_TextChanged"/>
        <Label Content="Dunwoody Housing Manager" HorizontalAlignment="Left" Margin="589,72,0,0" VerticalAlignment="Top" Width="167"/>
        <TextBox Name="password_box" HorizontalAlignment="Left" Height="23" Margin="589,254,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="180"/>
        <Button Name="login_btn" Content="Login" HorizontalAlignment="Left" Margin="638,326,0,0" VerticalAlignment="Top" Width="76" Foreground="White" Click="GoToSelectionWindow" Style="{StaticResource ButtonColor}"/>
    </Grid>
</Window>
